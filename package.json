{
  "name": "vue-dev-inspector",
  "version": "1.0.0",
  "description": "Developer mode inspector for Vue 3 and Nuxt 3 - annotate UI elements with API endpoints, DB fields, Figma URLs and more",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vue-dev-inspector": "./dist/cli.cjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./nuxt": {
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs",
      "types": "./dist/nuxt.d.ts"
    },
    "./cli": {
      "require": "./dist/cli.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src/components",
    "src/composables",
    "src/cli"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && npm run build:cli",
    "build:cli": "esbuild src/cli/index.ts --bundle --platform=node --outfile=dist/cli.cjs --format=cjs",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "lucide-vue-next": "^0.300.0"
  },
  "peerDependencies": {
    "pinia": "^2.0.0 || ^3.0.0",
    "vue": "^3.3.0",
    "xlsx": ">=0.18.0"
  },
  "peerDependenciesMeta": {
    "xlsx": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "esbuild": "^0.20.0",
    "lucide-vue-next": "^0.300.0",
    "pinia": "^2.1.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0",
    "xlsx": "^0.18.5"
  },
  "keywords": [
    "vue",
    "vue3",
    "nuxt",
    "nuxt3",
    "devtools",
    "inspector",
    "developer-mode",
    "annotations",
    "debug"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  }
}
